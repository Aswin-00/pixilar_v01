[Unit]

Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target
[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/pixilar
ExecStart=/home/ubuntu/box_venv/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/ubuntu/pixilar/pixilar.sock pixilar.wsgi:application

       
[Install]
WantedBy=multi-user.target